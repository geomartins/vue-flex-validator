{
  "name": "vue-flex-validator",
  "description": "A simple vuejs validator plugin",
  "version": "0.1.2",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-library": "vue-cli-service build --target lib --name vue-flex-validator ./src/install.js",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": ".dist/vue-flex-validator.common.js",
  "author": {
    "name": "Martins Abiodun",
    "email": "martinsabiodun94@gmail.com"
  },
  "repository": "https://github.com/geomartins/vue-flex-validator",
  "bugs": "https://github.com/geomartins/vue-flex-validator/issues",
  "keywords": [
    "vue3",
    "vue",
    "validator"
  ],
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
